            ],
            "application/vnd.google.colaboratory.intrinsic+json": {
              "type": "dataframe",
              "variable_name": "data",
              "repr_error": "Out of range float values are not JSON compliant: nan"
            }
          },
          "metadata": {},
          "execution_count": 19
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "def compute_zscore(row):\n",
        "    X = row[\"RawValue\"]\n",
        "    L = row[\"LowerLimit\"]\n",
        "    U = row[\"UpperLimit\"]\n",
        "    mu = row[\"Mean\"]\n",
        "    sd = row[\"SD\"]\n",
        "\n",
        "    if pd.notna(mu) and pd.notna(sd):\n",
        "        if sd <= 0:\n",
        "            return np.nan, \"InvalidSD\", \"INVALID_SD\"\n",
        "        z = (X - mu) / sd\n",
        "        method = \"DirectMeanSD\"\n",
        "\n",
        "    elif pd.notna(L) and pd.notna(U):\n",
        "        mu = (L + U) / 2\n",
        "        sd = (U - L) / 4\n",
        "        if sd <= 0:\n",
        "            return np.nan, \"InvalidSD\", \"INVALID_SD\"\n",
        "        z = (X - mu) / sd\n",
        "        method = \"DerivedFromRange\"\n",
        "\n",
        "    else:\n",
        "        return np.nan, \"MissingReference\", \"MISSING_RANGE\"\n",
        "\n",
        "    if abs(z) > 3:\n",
        "        return z, method, \"EXTREME_Z\"\n",
        "\n",
        "    return z, method, \"OK\"\n"
      ],
      "metadata": {
        "id": "z1wGklf0As_Z"
      },
      "execution_count": 20,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "z_out = data.apply(compute_zscore, axis=1, result_type=\"expand\")\n",
        "z_out.columns = [\"ZScore\", \"MethodUsed\", \"Flags\"]\n",
        "\n",
        "final_df = pd.concat(\n",
        "    [data[[\"CanonicalName\", \"RawValue\", \"Unit\"]], z_out],\n",
        "    axis=1\n",
        ")\n",
        "\n",
        "final_df\n"
      ],
